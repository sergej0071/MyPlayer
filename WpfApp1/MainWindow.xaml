<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow"  
        AllowsTransparency="True" 
        Height="330" Width="800" MinHeight="200" MinWidth="880"  
        Icon="image/бак121.png" Background="White"  ResizeMode="CanResizeWithGrip"
        MouseLeftButtonDown="Window_MouseLeftButtonDown" WindowStyle="None">

    <Window.Resources>


        <Style x:Key="SliderRepeatButton" TargetType="RepeatButton">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="IsTabStop" Value="false" />
            <Setter Property="Focusable" Value="false" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border  BorderThickness="1" BorderBrush="Black" Background="Black" Height="3"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="SliderRepeatButton1" TargetType="RepeatButton">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border SnapsToDevicePixels="True" Background="Green"  BorderThickness="1" BorderBrush="YellowGreen" Height="3"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="MyShapeStyle" TargetType="Shape">
            <Setter Property="Fill" Value="{DynamicResource MyFillBrush}" />
        </Style>       
        

        <Style x:Key="SliderThumbStyle" TargetType="Thumb">
            <Setter Property="SnapsToDevicePixels" Value="true"/>
            <Setter Property="OverridesDefaultStyle" Value="false"/>
            <Setter Property="Height" Value="18"/>
            <Setter Property="Width" Value="18"/>

            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Thumb">

                        <Ellipse Style="{StaticResource MyShapeStyle}" Width="20"
                          Stroke="Black"
                          
                          Name="Ellipse" 
                          Margin="-20,0,-17.5,0" Fill="#FF742ADE"
                        />
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="Ellipse" Property="Fill" Value="#FFD8B6B6"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter TargetName="Ellipse" Property="Fill" Value="#FFD8B6B6"/>
                            </Trigger>


                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        

        <Style TargetType="Slider" x:Key="AppSliderStyle4">
            <Setter Property="OverridesDefaultStyle" Value="true"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Slider">
                        <ControlTemplate.Resources>
                            <Storyboard x:Key="OnMouseEnter1">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="thumb">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FFF0F0F0"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.1" Value="#FFECE3E3"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFAEA0A0"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                        </ControlTemplate.Resources>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" Name="row" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" Name="column" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>
                            <Border Name="PART_Border"
                           BorderBrush="{x:Null}" BorderThickness="0"
                           Padding="2"
                           Grid.Row="1" Grid.Column="1"
                           Width="{TemplateBinding Width}"
                           Height="8"
                           Background="#FF503768"
                           HorizontalAlignment="Stretch"
                           VerticalAlignment="Center" />

                            <Track Name="PART_Track"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              Grid.Row="1" Grid.Column="1"
                              Width="{TemplateBinding Width}"
                              Height="{TemplateBinding Height}"> 
                               
                                <Track.DecreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource SliderRepeatButton1}"  Command="Slider.DecreaseLarge" />
                                </Track.DecreaseRepeatButton>
                                
                                <Track.IncreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource SliderRepeatButton}" Command="Slider.IncreaseLarge" />
                                </Track.IncreaseRepeatButton>                  


                              
                                <Track.Thumb>
                                    <Thumb x:Name="thumb" Style="{StaticResource SliderThumbStyle}" />
                                </Track.Thumb>
                            </Track>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                <BeginStoryboard Storyboard="{StaticResource OnMouseEnter1}"/>
                            </EventTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <!--кнопка свернуть  ButtonStyleCollapse  -->

        <Style x:Key="ButtonStyleCollapse" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <ControlTemplate.Resources>
                            <Storyboard x:Key="OnMouseEnter1_"/>
                            <Storyboard x:Key="minimize_button_enter">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF27004D"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="minimaze_button_leave">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF27004D"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                        </ControlTemplate.Resources>
                        <Grid>
                            <Rectangle x:Name="rectangle" Width="42.166" VerticalAlignment="Top" Stroke="#FF27004D" Height="30.333" HorizontalAlignment="Left" Fill="#FF27004D"/>
                            <Path Width="14.833" VerticalAlignment="Top" Stroke="White" Stretch="Fill" Margin="14.667,15.832,0,0" Height="1" HorizontalAlignment="Left" Fill="#FFF1F1F1" Data="M782.18215,-23.746735 L798.80691,-23.746735"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                <BeginStoryboard x:Name="minimize_button_enter_BeginStoryboard" Storyboard="{StaticResource minimize_button_enter}"/>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                <BeginStoryboard x:Name="minimaze_button_leave_BeginStoryboard" Storyboard="{StaticResource minimaze_button_leave}"/>
                            </EventTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--Полноэкранный режим  -->

        <Style x:Key="EnterButtonFullScreen" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <ControlTemplate.Resources>
                            <Storyboard x:Key="collapse_button_enter">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF27004D"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle1">
                                    <EasingColorKeyFrame KeyTime="0:0:0.1" Value="sc#1, 0.0407673046, 0.00167326792, 0.1468699"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" Storyboard.TargetName="rectangle1">
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="13.6425"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="179.642"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="0"/>
                                </DoubleAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="collapse_button_leave">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF27004D"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle1">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.1" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="sc#1, 0.0407673046, 0.00167326792, 0.1468699"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.5" Value="#FF27004D"/>
                                </ColorAnimationUsingKeyFrames>
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" Storyboard.TargetName="rectangle1">
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="179.642"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="13.6425"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="-0.612"/>
                                </DoubleAnimationUsingKeyFrames>
                            </Storyboard>


                        </ControlTemplate.Resources>
                        <Grid>
                            <Rectangle x:Name="rectangle" Width="42.166" VerticalAlignment="Top" Stroke="#FF27004D" Height="30.333" HorizontalAlignment="Left" Fill="#FF27004D"/>
                            <Rectangle Width="14.833" VerticalAlignment="Top" Stroke="White" Margin="15.668,7.831,0,0" Height="12.667" HorizontalAlignment="Left"/>
                            <Rectangle x:Name="rectangle1" Width="14.833" VerticalAlignment="Top" Stroke="White" Margin="13.001,10.498,0,0" Height="12.667" HorizontalAlignment="Left" Fill="#FF27004D" RenderTransformOrigin="0.5,0.5">
                                <Rectangle.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Rectangle.RenderTransform>
                            </Rectangle>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                <BeginStoryboard Storyboard="{StaticResource collapse_button_enter}"/>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                <BeginStoryboard x:Name="collapse_button_leave_BeginStoryboard" Storyboard="{StaticResource collapse_button_leave}"/>
                            </EventTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--Закрыть приложение -->

        <Style x:Key="ButtonStyleClose" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <ControlTemplate.Resources>
                            <Storyboard x:Key="Mouse_Enter_Close">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF27004D"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="Mouse_Leave_Close">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF27004D"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                        </ControlTemplate.Resources>
                        <Grid>
                            <Rectangle x:Name="rectangle" Width="42.166" VerticalAlignment="Top" Stroke="#FF27004D" Height="30.333" HorizontalAlignment="Left" Fill="#FF27004D"/>
                            <Path Width="14.833" VerticalAlignment="Top" Stroke="White" Stretch="Fill" Margin="13.833,9.163,0,0" Height="12.667" HorizontalAlignment="Left" Fill="#FFF1F1F1" Data="M893.33333,-32.666667 L912.66667,-13.666667"/>
                            <Path Width="14.833" VerticalAlignment="Top" Stroke="White" Stretch="Fill" Margin="13.833,9.163,0,0" Height="12.667" HorizontalAlignment="Left" Fill="#FFF1F1F1" Data="M911.5,-32.667 L893.333,-17"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                <BeginStoryboard Storyboard="{StaticResource Mouse_Enter_Close}"/>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                <BeginStoryboard x:Name="Mouse_Leave_Close_BeginStoryboard" Storyboard="{StaticResource Mouse_Leave_Close}"/>
                            </EventTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!---->




        <Style TargetType="Button" x:Key="Button_Navigation_Form">
            <Setter Property="MinHeight" Value="25"/>
            <Setter Property="MinWidth" Value="25"/>
            <Setter Property="MaxHeight" Value="25"/>
            <Setter Property="MaxWidth" Value="25"/>
        </Style>

        <Style TargetType="Button" x:Key="Button_Track_Navigation">
            <Setter Property="Height" Value="40" />
            <Setter Property="Width" Value="40"/>
            <Setter Property="Margin" Value="10,0,0,0"/>

        </Style>


        <Style x:Key="ButtonStyle1" TargetType="{x:Type Button}"/>

        <Style x:Key="ButtonStyle2" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid>
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualStateGroup.Transitions>
                                        <VisualTransition From="Normal" GeneratedDuration="0" To="MouseOver">
                                            <Storyboard>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                                    <EasingColorKeyFrame KeyTime="0" Value="#FF340264"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF340264"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF5C189E"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.5" Value="#FF7520C7"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.6" Value="#FF7520C7"/>
                                                </ColorAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualTransition>
                                        <VisualTransition From="MouseOver" GeneratedDuration="0" To="Normal">
                                            <Storyboard>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                                    <EasingColorKeyFrame KeyTime="0" Value="#FF340264"/>

                                                    <EasingColorKeyFrame KeyTime="0:0:0.6" Value="#FF7520C7"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.5" Value="#FF7520C7"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF5C189E"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF340264"/>
                                                </ColorAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualTransition>
                                        <VisualTransition From="MouseOver" GeneratedDuration="0" To="Pressed">
                                            <Storyboard>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                                    <EasingColorKeyFrame KeyTime="0:0:0.6" Value="#FF7520C7"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.5" Value="#FF7520C7"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF5C189E"/>
                                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF340264"/>
                                                </ColorAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualTransition>
                                    </VisualStateGroup.Transitions>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Rectangle x:Name="rectangle" Width="39.334" VerticalAlignment="Top" Height="28.667" HorizontalAlignment="Left" Fill="#FF27004D"/>
                            <Path Width="22.808" VerticalAlignment="Top" Stroke="WhiteSmoke" Stretch="Fill" Margin="9.014,14.042,0,0" Height="1" HorizontalAlignment="Left" Fill="#FFF7F7F7" Data="M917.375,19.75 L942.75,18"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsFocused" Value="True"/>
                            <Trigger Property="IsDefaulted" Value="True"/>
                            <Trigger Property="IsMouseOver" Value="True"/>
                            <Trigger Property="IsPressed" Value="True"/>
                            <Trigger Property="IsEnabled" Value="False"/>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--Режим повтора трека  -->

        <Style x:Key="ButtonStyleReplayTrack"  TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <ControlTemplate.Resources>
                            <Storyboard x:Key="Mouse_Enter">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle1">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF3E007C"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF3E007C"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle1">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF27004D"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle2">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle3">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle3">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle2">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF503768"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="Mouse_Leave">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle1">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF3E007C"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF3E007C"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle1">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF27004D"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle2">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF3E007C"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle2">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF3E007C"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle3">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF3E007C"/>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="rectangle3">
                                    <EasingColorKeyFrame KeyTime="0" Value="#FF503768"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF460B81"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FF3E007C"/>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>


                            <Storyboard x:Key="Mouse_Click" SpeedRatio="0.2" RepeatBehavior="1x" AutoReverse="True" >
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="rectangle3">
                                    <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.061" Value="-1.733"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.125" Value="-3.979"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.25" Value="-7.292"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.375" Value="-10.94"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.389" Value="-11.042"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="-8.506"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.625" Value="-7.836"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.75" Value="-7.166"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-7.596"/>
                                </DoubleAnimationUsingKeyFrames>
