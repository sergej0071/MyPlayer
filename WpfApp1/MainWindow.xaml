    <Window.Triggers>
        <EventTrigger RoutedEvent="UIElement.MouseEnter"/>
        <EventTrigger RoutedEvent="UIElement.MouseLeave"/>
    </Window.Triggers>





    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FF232121" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>






        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="25"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="116"/>
            <ColumnDefinition Width="107*"/>
            <ColumnDefinition Width="199"/>
            <ColumnDefinition Width="102*"/>
            <ColumnDefinition Width="147"/>
        </Grid.ColumnDefinitions>

        <!-- имя плеера -->
        <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal" Margin="3,3,0,0" Background="#FF27004D">
            <TextBlock Text="MyPlayer" Margin="15,5,5,5" Foreground="#FFE8E3E3" FontSize="16" />
            <TextBlock/>
        </StackPanel>

        <!-- имя трека -->
        <StackPanel Grid.Column="1" Grid.Row="0" Orientation="Horizontal" Grid.ColumnSpan="3"  Margin="0,3,0,0" Background="#FF27004D">
            <TextBlock  x:Name="NameTrackUp" FontSize="15" Text="" Foreground="#FFE8E3E3" Margin="30,5,0,0" />

        </StackPanel>

        <!-- навигация -->
        <StackPanel Grid.Column="4" Orientation="Horizontal" Margin="0,3,3,0" Background="#FF27004D" >
            <Button Margin="12,0,0,0" Style="{DynamicResource ButtonStyleCollapse}" Click="Сollapse_Player" />
            <Button Margin="0,0,0,0" Style="{DynamicResource EnterButtonFullScreen}" Click="full_screen_Player"/>
            <Button Margin="0,0,0,0" Style="{DynamicResource ButtonStyleClose}" Click="Close_Player"/>
        </StackPanel>



        <!--element-->
        <MediaElement x:Name="PlayerElement"  Grid.Column="0" Grid.Row="1" LoadedBehavior="Manual" Grid.ColumnSpan="5" MediaOpened="PlayerElement_MediaOpened" Margin="0,5,0,0" />

        <!-- Картинка -->
        <StackPanel Grid.Column="0" Grid.Row="2" Grid.RowSpan="2">
            <TextBlock/>
        </StackPanel>

        <!-- Картинка -->
        <StackPanel Grid.Column="0" Grid.Row="2" Margin="3,0,0,0" Background="#FF3E007C">
            <TextBlock/>
        </StackPanel>

        <!--время трек назв -->
        <StackPanel  Grid.Column="1" Grid.Row="2" Orientation="Vertical" Grid.ColumnSpan="3" Background="#FF3E007C">
            <StackPanel Orientation="Horizontal">
                <TextBlock x:Name="CurrentTime" Text="00:00" FontSize="30" Foreground="#FFE8E3E3" Margin="30,10,0,0" />
                <TextBlock  x:Name="MaxTimeAudioN" Text="00:00" FontSize="18" Foreground="#FFE8E3E3" Margin="20,23,0,0" />
            </StackPanel>
            <TextBlock x:Name="NameTrack" Text="" FontSize="25" Margin="30,0,5,0" Foreground="#FFE8E3E3"/>

        </StackPanel>

        <!--действие с треком -->
        <StackPanel  Grid.Column="4" Grid.Row="2" Orientation="Horizontal" Margin="0,0,3,0" Background="#FF3E007C">
            <Button x:Name="replay"  Margin="20,10,3,0" Style="{ DynamicResource ButtonStyleReplayTrack}" Click="RePlay_Track" Height="30" />
            <Button x:Name="forward_track"  Margin="0,10,3,0" Style="{DynamicResource ButtonStyle7}" Click="Forward_Track" Height="30" />

        </StackPanel>

        <!--трек -->
        <StackPanel  Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="5" Margin="3,0,3,0" Background="#FF3E007C">
            <Slider  x:Name="TrackSlider" Style="{DynamicResource AppSliderStyle4}" IsMoveToPointEnabled="True" Height="20" Margin="14,0,6,0" Minimum="0" Thumb.DragCompleted="MoveThumb" ValueChanged="TrackValueChanged" MouseLeftButtonUp="TrackSlider_MouseLeftButtonUp" MouseLeftButtonDown="TrackSlider_MouseLeftButtonDown" IsMouseCaptureWithinChanged="TrackSlider_IsMouseCaptureWithinChanged">
                <Slider.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="Black" Offset="0"/>
                        <GradientStop Color="#FFCD5757" Offset="1"/>
                    </LinearGradientBrush>
                </Slider.Background>
            </Slider>
        </StackPanel>

        <!-- загрузка путей треков -->
        <StackPanel Grid.Column="0" Orientation="Horizontal" Grid.Row="4" Margin="3,3,0,3" Background="#FF27004D">
            <TextBlock x:Name="CurrentTimeD" Text="00:00" FontSize="16" Margin="10,0,0,0" Foreground="#FFE8E3E3"/>



        </StackPanel>

        <StackPanel Grid.Column="1" Grid.Row="4" Margin="0,3,0,3" Background="#FF27004D"/>



        <!-- навигация по трекам -->
        <StackPanel  Grid.Column="2" Grid.Row="4" Orientation="Horizontal" Grid.ColumnSpan="2" Margin="0,3,0,3" Background="#FF27004D">
            <Button Style="{DynamicResource ButtonStyle6}"  Click="TreakLoading" Height="40" Width="40"/>
            <Button   Style="{DynamicResource ButtonStyle3}" Click="Back" Height="40" />
            <Button x:Name="ButtonPlay" Content="True"  Style= "{DynamicResource ButtonStyle4}" Click="Play" Height="40"/>
            <Button   Style="{DynamicResource ButtonStyle3}" Click="forward" RenderTransformOrigin="0.5,0.5" Height="40">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleY="1" ScaleX="-1"/>
                        <SkewTransform AngleY="0" AngleX="0"/>
                        <RotateTransform Angle="0"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
            </Button>
            <Button  Style="{DynamicResource ButtonStyle5}" Click="Stop" Height="40"/>
        </StackPanel>


        <StackPanel Grid.Column="3" Grid.Row="4" Margin="0,3,0,3" Background="#FF27004D"/>

        <!-- громкость  и конечное время -->
        <StackPanel Grid.Column="4" Grid.Row="4" Margin="0,3,3,3" Background="#FF27004D">
            <TextBlock x:Name="MaxTimeAudioV" Text="00:00" Foreground="#FFE8E3E3" Margin="90,0,0,0" FontSize="16"/>
            <Slider Style="{DynamicResource AppSliderStyle4}" IsMoveToPointEnabled="True" Maximum="1" Minimum="0" x:Name="Volume" ValueChanged="VolumeValueChanged"/>
        </StackPanel>
    </Grid>
</Window>
    
    
